{
    "logging": {
        "level": "INFO",
        "format": "[%(levelname)s] %(message)s",
        "colors": {
            "DEBUG": "\u001b[34m",
            "INFO": "\u001b[32m",
            "WARNING": "\u001b[33m",
            "ERROR": "\u001b[31m",
            "RESET": "\u001b[0m"
        }
    },
    "paths": {
        "log_file": "logs/app.log"
    }
}
